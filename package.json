{
  "name": "jsinterview",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "dev:poll": "WEBPACK_POLLING=1000 next dev",
    "dev:windows": "set WEBPACK_POLLING=1000 && next dev",
    "fix:watchers": "sudo ./scripts/fix-watchers.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check:vars": "npx tsx scripts/check-vars.ts",
    "check:keyframes": "npx tsx scripts/check-keyframes.ts",
    "tokens:audit": "npx tsx scripts/tokens-audit.ts",
    "test:visual": "npx playwright test",
    "test:visual:update": "npx playwright test --update-snapshots",
    "prepare": "husky"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "acorn": "^8.11.3",
    "acorn-walk": "^8.3.2",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "immer": "^10.0.4",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "postcss-value-parser": "^4.2.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.5.0",
    "vitest": "^4.0.16"
  }
}
